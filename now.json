{
  "version": 2,
  "build": {
    "env": {
      "GATSBY_SOURCE_GRAPHQL": "@gatsby_source_graphql",
      "GATSBY_GITHUB_TOKEN": "@gatsby_github_token",
      "GATSBY_FATHOM_TRACKING_ID_SCOTT_SPENCE_ME": "@gatsby_fathom_tracking_id_scott_spence_me"
    }
  },
  "redirects": [
    {
      "source": "/about",
      "destination": "/#hi-im-scott"
    },
    {
      "source": "/portfolio",
      "destination": "/#portfolio"
    },
    {
      "source": "/now",
      "destination": "/#what-im-doing-now"
    },
    {
      "source": "/uses",
      "destination": "/#uses"
    },
    {
      "source": "/#hi-im-scott",
      "destination": "https://scottspence.com/#hi-im-scott"
    },
    {
      "source": "/#portfolio",
      "destination": "https://scottspence.com/#portfolio"
    },
    {
      "source": "/#now",
      "destination": "https://scottspence.com/#what-im-doing-now"
    },
    {
      "source": "/#uses",
      "destination": "https://scottspence.com/#uses"
    },
    {
      "source": "/faq",
      "destination": "https://scottspence.com/faq"
    }
  ],
  "trailingSlash": false
}
